                for(i = 0; i < data.Message.length; i++)
                {
                    if(data.Message[i].id != @Html.Raw(gorev.id) && 1 == 1)
                    {
                        $("#bagli_gorev").append('<option value="' + data.Message[i].id + '">' + data.Message[i].isim + '</option>');
