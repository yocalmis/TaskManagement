@model List<GorevYoneticisi.Models.musteriler>
@{
    ViewBag.Title = "Müşteriler/Mükellefler";
    Layout = "~/Areas/Admin/Views/Shared/AdminLayout.cshtml";
}

<div class="top">
    <i class="fa fa-ellipsis-v" aria-hidden="true"></i>
    <h2>Müşteriler/Mükellefler</h2>
</div>

<div class="ilan-list">
    <div class="head clearfix">
        <h4 class="col-sm-4">MÜŞTERİ/MÜKELLEF FİRMA</h4>
        <h4 class="col-sm-4">YETKİLİ</h4>
        <h4 class="col-sm-2">DURUM</h4>
        <h4 class="col-sm-2"></h4>
    </div>
    @foreach (GorevYoneticisi.Models.musteriler mstr in Model)
    {
        <div class="satir clearfix">
            <div class="col-sm-4">@Html.Raw(mstr.firma_adi)</div>
            <div class="col-sm-4">@Html.Raw(mstr.ad + " " + mstr.soyad)</div>
            <div class="col-sm-2">@Html.Raw(GorevYoneticisi.Tools.durumlar.getDurum(mstr.flag))</div>
            <div class="col-sm-2" style="margin-top:-5px;">
                <a id="teklif-button" href="@Html.Raw(Url.Action("Musteri", new { id = mstr.url }))"><i class="fa fa-pencil" aria-hidden="true"></i></a>
            </div>
        </div>

    }
</div>







